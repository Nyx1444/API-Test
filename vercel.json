{
    "version": 2,
    "builds": [
      {
        "src": "api/users.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/users", "dest": "/api/users.js" },
      { "src": "/style.css", "dest": "/public/style.css" },
      { "src": "/script.js", "dest": "/public/script.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  